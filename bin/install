#!/bin/bash

# install homebrew.....
echo 'getting homebrew......'
/usr/bin/ruby -e "$(curl -fsSL https://raw.githubusercontent.com/Homebrew/install/master/install)" > /dev/null

echo 'dotnet stuff........'
brew cask install dotnet-sdk visual-studio-code > /dev/null

echo 'go stuff........'
brew install go > /dev/null

echo 'ide adjacent things'
brew install zsh vim tmux reattach-to-user-namespace > /dev/null

echo 'random stuff (git, jq, etc)'
brew install jq git awscli > /dev/null

echo 'chrome, slack, etc'
brew cask install docker google-chrome slack
